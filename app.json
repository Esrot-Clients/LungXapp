{
  "expo": {
    "name": "Krakel",
    "slug": "krakenlungx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/kaLog.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0",
      "bundleIdentifier": "y",
      "infoPlist": {
        "NSBluetoothAlwaysUsageDescription": "We need Bluetooth access to connect to your audio devices",
        "NSBluetoothPeripheralUsageDescription": "We need Bluetooth access to connect to your audio devices"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 3,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "package": "com.centrelocus.KrakenLungX",
      "targetSdkVersion": 34
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "23f02aab-3cbe-4543-abc1-d11ba3622b52"
      }
    }
  }
}
